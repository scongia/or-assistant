{
  "name": "or-assistant",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": "^8.9"
  },
  "files": [
    "db",
    "srv"
  ],
  "dependencies": {
    "@sap/cds": "3.33.1",
    "@sap/cds-odata-v2-adapter-proxy": "^1.4.22",
    "@sap/hana-client": "^2.4.191",
    "@sap/logging": "^5.2.0",
    "compression": "^1.7.4",
    "express": "^4",
    "helmet": "^3.22.0",
    "mysql": "^2.18.1",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "sqlite3": "^4.1.1"
  },
  "scripts": {
    "setup": "npm install && npm run deploy",
    "deploy": "cds deploy srv --to sqlite:db/or-assistant.db",
    "build": "cds build/all --clean",
    "start": "npx cds run",
    "setup:build:deploy": "npm run build && npm run build && npm run deploy",
    "build:deploy:startv2": "npm run build && npm run deploy && node v2server",
    "startv2": "node v2server"
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "sqlite",
        "model": "srv",
        "credentials": {
          "database": "db/or-assistant.db"
        }
      }
    },
    "odata": {
      "version": "v4"
    }
  }
}