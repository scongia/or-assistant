{
  "name": "or-assistant",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": "^8.9"
  },
  "files": [
    "db",
    "srv"
  ],
  "dependencies": {
    "@sap/cds": "^3",
    "@sap/cds-odata-v2-adapter-proxy": "^1.4.22",
    "@sap/logging": "^5.2.0",
    "express": "^4"
  },
  "devDependencies": {
    "sqlite3": "^4.1.1"
  },
  "scripts": {
    "setup": "npm install && npm run deploy",
    "deploy": "cds deploy srv --to sqlite:db/or-assistant.db",
    "build": "cds build/all --clean",
    "start": "npx cds run",
    "build:deploy:startv2": "npm run build && npm run deploy && node v2server",
    "startv2": "node v2server"
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "sqlite",
        "model": "srv",
        "credentials": {
          "database": "db/or-assistant.db"
        }
      }
    },
    "odata": {
      "version": "v4"
    }
  }
}