{
  "welcomeFile": "/app/fiori.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "/admin/(.*)$",
      "target": "/admin/$1",
      "authenticationType": "xsuaa",
      "destination": "srv_api",
      "csrfProtection": true
    },
    {
      "source": "/v2/(.*)$",
      "target": "/v2/$1",
      "authenticationType": "xsuaa",
      "destination": "srv_api",
      "csrfProtection": true
    },
    {
      "source": "^/app/(.*)$",
      "target": "$1",
      "localDir": "webapp"
    },
    {
      "source": "^/appconfig/(.*)",
      "target": "/appconfig/$1",
      "localDir": "webapp"
    }
  ]
}